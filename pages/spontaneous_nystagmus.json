          "position": {"row": 1, "column": 1}
        },
        {
          "key": "固视抑制",
          "type": "radio",
          "label": "固视抑制",
          "values": ["阴性", "阳性"],
          "autofill_when": {"key": "眼震模式", "equals": "阴性", "value": "N/A"},
          "order": 2,
          "position": {"row": 2, "column": 0}
        },
        {
          "key": "结果判读",
          "type": "radio",
          "label": "结果判读",
          "values": ["正常", "异常"],
          "required": true,
          "order": 6,
          "position": {"row": 2, "column": 1}
        }
      ]
    }
  ]
